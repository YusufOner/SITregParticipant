<core:FragmentDefinition
		xmlns="sap.m"
		xmlns:core="sap.ui.core">

	<ViewSettingsDialog
			id="viewSettingsDialog"
			confirm="onConfirmViewSettingsDialog"
			resetFilters="onViewSettingsDialogResetFilters">
		<filterItems>
			<ViewSettingsFilterItem
					id="filterItems"
					text="{i18n>masterFilterName}"
					key="MaxParticipants"
					multiSelect="false">
				<items>
					<ViewSettingsItem
							text="{i18n>masterFilter1}"
							key="Filter1"
							id="viewFilter1"/>
					<ViewSettingsItem
							text="{i18n>masterFilter2}"
							key="Filter2"
							id="viewFilter2"/>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
					id="filterItems1"
					text="{i18n>masterFilterNameType}"
					key="Type"
					multiSelect="true"
					items="{/EventType}">
				<items>
					<ViewSettingsItem
							text="{Description}"
							key="{EventType}"/>
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem
					id="filterItems2"
					text="{i18n>masterFilterVisible}"
					key="Visible"
					multiSelect="false">
				<items>
					<ViewSettingsItem
							text="{i18n>yes}"
							key="Y"
							id="viewFilterYes"/>
					<ViewSettingsItem
							text="{i18n>no}"
							key="N"
							id="viewFilterNo"/>
				</items>
			</ViewSettingsFilterItem>
		</filterItems>
	</ViewSettingsDialog>

</core:FragmentDefinition>