<mvc:View 	controllerName="com.sap.sapmentors.sitreg.registration.controller.CreateEntity" 	xmlns:core="sap.ui.core" xmlns:f="sap.ui.layout.form"	xmlns:footerbar="sap.ushell.ui.footerbar" xmlns:l="sap.ui.layout" xmlns:mvc="sap.ui.core.mvc" xmlns:semantic="sap.m.semantic"	xmlns="sap.m">	<semantic:FullscreenPage		id="page"		title="{i18n>createViewTitle}"		showNavButton="true"		navButtonPress="onNavBack">		<semantic:content>		<f:SimpleForm id="SimpleFormParticipant"			minWidth="1024"			maxContainerCols="2"			editable="true"			layout="ResponsiveGridLayout"			title="Participant Registration"			labelSpanL="4"			labelSpanM="4"			emptySpanL="0"			emptySpanM="0"			columnsL="2"			columnsM="2"			class="editableForm">			<f:content>				<core:Title text="Visible for Everybody" />				<Label text="{i18n>FirstNameLabel}" />				<Input value="{FirstName}" />				<Label text="{i18n>LastNameLabel}" />				<Input value="{LastName}"/>				<Label text="{i18n>BioURLLabel}" />				<Input value="{BioURL}" />				<Label text="{i18n>TwitterLabel}" />				<Input value="{Twitter}" />				<Label text="{i18n>PreEveningEventLabel}" />				<HBox >					<RadioButtonGroup columns="5" select="onSelectPreEveningEvent">						<buttons>							<RadioButton id="YPRE" groupName="PreEveningEventRbt" text="Yes"    selected="{= ${PreEveningEvent} === 'Y' ? true : false}" value="Y" />							<RadioButton id="NPRE" groupName="PreEveningEventRbt" text="No"     selected="{= ${PreEveningEvent} === 'N' ? true : false}" value="N"/>							<RadioButton id="MPRE" groupName="PreEveningEventRbt" text="Maybe"  selected="{= ${PreEveningEvent} === 'M' ? true : false}" value="M"/>						</buttons>					</RadioButtonGroup>									</HBox>				<Label text="{i18n>PostEveningEventLabel}" />				<HBox>					<RadioButtonGroup columns="5" select="onSelectPostEveningEvent">						<buttons>							<RadioButton id="YPost" groupName="PostEveningEventRbt" text="Yes" 	 selected="{= ${PostEveningEvent} === 'Y' ? true : false}"  value="Y"/>							<RadioButton id="NPost" groupName="PostEveningEventRbt" text="No"  	 selected="{= ${PostEveningEvent} === 'N' ? true : false}"  value="N"/>							<RadioButton id="MPost" groupName="PostEveningEventRbt" text="Maybe" selected="{= ${PostEveningEvent} === 'M' ? true : false}"  value="M"/>						</buttons>					</RadioButtonGroup>						</HBox>								<core:Title text="Visible Only for Organizers" />				<Label text="{i18n>EmailLabel}" />				<Input value="{EMail}" />				<Label text="{i18n>MobilePhoneLabel}" />				<Input value="{MobilePhone}" />				<Label text="{i18n>RSVPLabel}" />				<Input value="{RSVP}" />				<!--				<CheckBox selected="{ path: 'RSVP',  formatter : '.formatter.rsvpSwitchValue' }" text="{i18n>RSVPText}" />				<Switch type="AcceptReject" state="{ path: 'RSVP',  formatter : '.formatter.rsvpSwitchValue' }" />				-->				<Label text="{i18n>RelationToSAPLabel}" />				<Input value="{RelationToSAP}" />				<Label text="{i18n>ReceiptLabel}" />				<CheckBox selected="{Receipt}" text="{i18n>ReceiptwishLabel}" />				<Label text="{i18n>ReceiptCompanyLabel}" />				<Input value="{ReceiptCompany}" maxLength="256"/>				<Label text="{i18n>ReceiptAddressLabel}" />				<TextArea growing="false" rows="5"  width="100%" value="{ReceiptAddress}" maxLength="512"/>							</f:content>		</f:SimpleForm>		</semantic:content>		<semantic:saveAction>			<semantic:SaveAction id="save" press="onSave"/>		</semantic:saveAction>		<semantic:cancelAction>			<semantic:CancelAction id="cancel" press="onCancel"></semantic:CancelAction>		</semantic:cancelAction>	</semantic:FullscreenPage></mvc:View>